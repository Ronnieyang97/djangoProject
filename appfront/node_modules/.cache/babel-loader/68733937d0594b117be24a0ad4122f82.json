employee.investment_adviser[i]['picture']\n              employee.investment_adviser[i].active = false\n            }\n          })\n    })\n\n    return {employee}\n  }\n}\n</script>\n<style>\n.team_employee {\n  width: auto;\n  height: auto;\n  align-items: center;\n  justify-content: center;\n  max-width: 20.8vw;\n}\n\n.image {\n  width: 20.8vw;\n  height: auto;\n  max-width: 20.8vw;\n}\n\n.team_employee_name {\n  z-index: 2;\n  position: absolute;\n  bottom: 3vh;\n  left: 2vw;\n}\n\n.image:hover {\n  opacity: 0.5;\n}\n\n</style>"],"sourceRoot":""}]}