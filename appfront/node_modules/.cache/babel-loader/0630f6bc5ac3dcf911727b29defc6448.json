,EAAwB,OAAxB,CAAd;AACA,MAAA,KAAK,CAAC,YAAN,GAAqB,UAAU,CAAC,KAAX,CAAiB,KAAjB,EAAwB,cAAxB,CAArB;AACA,MAAA,KAAK,CAAC,aAAN,GAAsB,UAAU,CAAC,KAAX,CAAiB,KAAjB,EAAwB,eAAxB,CAAtB;AACA,MAAA,KAAK,CAAC,SAAN,GAAkB,UAAU,CAAC,KAAX,CAAiB,KAAjB,EAAwB,WAAxB,CAAlB;AACA,MAAA,KAAK,CAAC,OAAN,GAAgB,IAAhB;AACF,KARA;;AASA,QAAM,KAAI,GAAI,SAAR,KAAQ,CAAC,CAAD,EAAO;AACnB,MAAA,OAAO,CAAC,GAAR,CAAY,CAAZ;AACA,MAAA,KAAK,CAAC,OAAN,GAAgB,KAAhB;AACF,KAHA;;AAIA,IAAA,SAAS,CAAC,YAAM;AACd,MAAA,KAAK,CAAC,GAAN,CAAU,sCAAV,EACK,IADL,CACU,UAAC,GAAD,EAAS;AACb,aAAK,IAAI,CAAA,GAAI,CAAb,EAAgB,CAAA,GAAI,GAAG,CAAC,IAAJ,CAAS,MAA7B,EAAqC,CAAC,EAAtC,EAA0C;AACxC,UAAA,UAAU,CAAC,KAAX,CAAiB,IAAjB,CAAsB,GAAG,CAAC,IAAJ,CAAS,CAAT,CAAtB;AACF;;AACA,aAAK,IAAI,EAAA,GAAI,CAAb,EAAgB,EAAA,GAAI,UAAU,CAAC,KAAX,CAAiB,MAArC,EAA6C,EAAC,EAA9C,EAAkD;AAChD,UAAA,UAAU,CAAC,KAAX,CAAiB,EAAjB,EAAoB,WAApB,IAAmC,0BAA0B,UAAU,CAAC,KAAX,CAAiB,EAAjB,EAAoB,WAApB,CAA7D;;AACA,cAAI,UAAU,CAAC,KAAX,CAAiB,EAAjB,EAAoB,eAApB,CAAJ,EAA0C;AACxC,YAAA,UAAU,CAAC,KAAX,CAAiB,EAAjB,EAAoB,eAApB,IAAuC,0BAA0B,UAAU,CAAC,KAAX,CAAiB,EAAjB,EAAoB,eAApB,CAAjE;AACF;AACF;AACD,OAXL;AAYD,KAbQ,CAAT;AAcA,WAAO;AAAC,MAAA,UAAU,EAAV,UAAD;AAAa,MAAA,KAAK,EAAL,KAAb;AAAoB,MAAA,SAAS,EAAT,SAApB;AAA+B,MAAA,KAAK,EAAL;AAA/B,KAAP;AACD;AA5CY,CAAf","sourcesContent":["<template>\n  <a-row type=\"flex\">\n    <a-col :span=\"2\"></a-col>\n    <a-col :span=\"5\" class=\"enterprise_page_title\">被投企业</a-col>\n    <a-col :span=\"17\"></a-col>\n  </a-row>\n  <a-row type=\"flex\" justify=\"center\">\n    <a-col v-for=\"(item, index) in enterprise.items\" :key=\"index\" class=\"enterprise_item\" @click=\"showmodal(index)\">\n      <img :src=\"item.trademark\">\n      <p style=\"color: grey; margin-top: 3vh\">{{ item.name }}</p>\n    </a-col>\n  </a-row>\n  <a-modal v-model:visible=\"modal.visible\" @click=\"close\"\n           :centered=\"true\" :footer=\"null\" :closable=\"false\" :width=\"'80vw'\"\n           :bodyStyle=\"{padding: 0,}\">\n    <div v-if=\"modal.picture_owner\" class=\"enterpirse_item_detail\">\n      <a-row>\n        <a-col :span=\"12\" style=\"display: flex\">\n          <img :src=\"modal.picture_owner\">\n          <div style=\"z-index: 2;position: absolute; padding:28vh 3vh\">\n            <p style=\"margin: 0; font-size: 4vh\">{{ modal.name }}</p>\n            <p>——</p>\n            <p style=\"font-size: 4vh\">{{ modal.owner }}</p>\n            <a style=\"color: white;font-size: 2.5vh\">{{ modal.site }}</a>\n          </div>\n        </a-col>\n        <a-col :span=\"12\">\n          <img :src=\"modal.trademark\"\n               style=\"width:100%; height:100%;max-width: 30vw;padding-top: 4vh;margin-left: 5vw;filter: brightness(100%)\">\n          <p style=\"font-size: 5vh;font-weight: bolder;color: black;text-align: center;margin-bottom: 2vh\">\n            {{ modal.name }}</p>\n          <img src=\"../../assets/image/line.png\"\n               style=\"width: 10vw;margin-left: 15vw;padding-bottom: 2vh;padding-top: 0\">\n          <p style=\"color: black; margin: 1vh 3vw\">公司官网 <a>{{ modal.site }}</a></p>\n          <p style=\"color: black; margin: 1vh 3vw;max-height: 30vh;overflow-y: scroll;\">{{ modal.introduction }}</p>\n        </a-col>\n      </a-row>\n    </div>\n    <div v-else>\n      <img :src=\"modal.trademark\" style=\"width: 30vw;margin-left: 25vw;\">\n      <p style=\"color: black;text-align: center;font-size:6vh;font-weight: bolder;margin-bottom: 2vh\">{{modal.name}}</p>\n      <img src=\"../../assets/image/line.png\" style=\"width: 20vw;margin-left: 30vw\">\n      <p style=\"color: black;overflow-x: hidden;padding: 5vw\">{{modal.introduction}}</p>\n    </div>\n  </a-modal>\n</template>\n\n<script>\nimport axios from 'axios'\nimport {reactive, onMounted} from 'vue'\n\nexport default {\n  name: 'Enterprise_page',\n  components: {},\n  setup() {\n    const enterprise = reactive({\n      items: []\n    })\n    let modal = reactive({\n      name: '',\n      site: '',\n      owner: '',\n      introduction: '',\n      picture_owner: '',\n      trademark: '',\n      visible: false,\n    })\n    const showmodal = (index) => {\n      modal.name = enterprise.items[index]['name']\n      modal.site = enterprise.items[index]['site']\n      modal.owner = enterprise.items[index]['owner']\n      modal.introduction = enterprise.items[index]['introduction']\n      modal.picture_owner = enterprise.items[index]['picture_owner']\n      modal.trademark = enterprise.items[index]['trademark']\n      modal.visible = true\n    }\n    const close = (e) => {\n      console.log(e)\n      modal.visible = false\n    }\n    onMounted(() => {\n      axios.get('http://127.0.0.1:8000/api/enterprise')\n          .then((res) => {\n            for (let i = 0; i < res.data.length; i++) {\n              enterprise.items.push(res.data[i])\n            }\n            for (let i = 0; i < enterprise.items.length; i++) {\n              enterprise.items[i][\"trademark\"] = 'http://localhost:8000' + enterprise.items[i][\"trademark\"]\n              if (enterprise.items[i][\"picture_owner\"]) {\n                enterprise.items[i][\"picture_owner\"] = 'http://localhost:8000' + enterprise.items[i][\"picture_owner\"]\n              }\n            }\n          })\n    })\n    return {enterprise, modal, showmodal, close}\n  },\n}\n</script>\n\n<style>\n.enterprise_item {\n  border-style: solid;\n  border-width: 1px;\n  border-color: #f8f8f8;\n  width: 20.8vw;\n  height: 25vh;\n}\n\n.enterprise_item img {\n  padding-top: 4vh;\n  width: auto;\n  height: auto;\n  max-width: 16vw;\n  -webkit-filter: grayscale(100%);\n  -moz-filter: grayscale(100%);\n  -ms-filter: grayscale(100%);\n  -o-filter: grayscale(100%);\n  filter: grayscale(100%);\n  filter: gray;\n}\n\n.enterprise_item img:hover {\n  -webkit-filter: inherit;\n  -moz-filter: inherit;\n  -ms-filter: inherit;\n  -o-filter: inherit;\n  filter: inherit;\n  filter: inherit;\n}\n\n.enterprise_page_title{\n    background: #2f54eb;\n  color: white;\n  font-size: 2.6vh;\n  height: 5.4vh;\n  text-align: left;\n  line-height: 5.4vh;\n  padding-left: 2vh;\n  text-indent: 2em;\n}\n.enterpirse_item_detail {\n\n}\n\n.enterpirse_item_detail img {\n  width: 100%;\n  height: 100%;\n  max-width: 40vw;\n  filter: brightness(80%)\n}\n</style>"],"sourceRoot":""}]}