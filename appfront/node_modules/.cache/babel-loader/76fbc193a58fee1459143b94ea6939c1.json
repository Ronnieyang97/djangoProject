tal\" class=\"header-bar\">\n    <a-menu-item style=\"line-height:8vh;width: 12vw\">\n      <router-link :to=\"'/index'\">\n        <img src=\"../assets/logo.png\">\n      </router-link>\n    </a-menu-item>\n    <a-menu-item v-for=\"(item, index) in routers\" :key=\"index\" class=\"item\">\n      <router-link :to=\"item.url\" @click.passive=\"backtotop\">{{ item.title }}</router-link>\n    </a-menu-item>\n  </a-menu>\n  <router-view></router-view>\n</template>\n\n\n<script>\nimport {reactive} from 'vue'\n\nexport default {\n  name: 'Header',\n  setup() {\n    const routers = reactive([\n      {url: '/index', title: '首页'},\n      {url: '/concept', title: '投资理念'},\n      {url: '/team', title: '投资团队'},\n      {url: '/enterprise', title: '被投企业'},\n      {url: '/news', title: '公司新闻'},\n      {url: '/contact', title: '联系我们'},\n    ])\n    const backtotop = () => {\n      document.documentElement.scrollTo(0, 0)\n    }\n    return {routers, backtotop}\n  }\n}\n</script>\n\n<style>\n.item {\n  width: 12vw;\n  line-height: 8vh;\n}\n\n.header-bar{\n  font-weight: bold;\n  height: 8vh;\n      list-style-type: none;\n    margin: 0;\n    padding: 0;\n    overflow: hidden;\n    position: fixed;\n    top: 0;\n    width: 100%;\n  z-index: 999;\n}\n\n</style>\n"],"sourceRoot":""}]}