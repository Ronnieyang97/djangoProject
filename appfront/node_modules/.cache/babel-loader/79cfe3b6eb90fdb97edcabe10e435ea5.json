,SAAlB,IAA+B,0BAA0B,QAAQ,CAAC,KAAT,CAAe,EAAf,EAAkB,SAAlB,CAAzD;AACA,UAAA,QAAQ,CAAC,KAAT,CAAe,EAAf,EAAkB,MAAlB,GAA2B,KAA3B;AACF;AACD,OAbL;AAcD,KAfQ,CAAT;AAiBA,WAAO;AAAC,MAAA,QAAQ,EAAR;AAAD,KAAP;AACF;AAzBa,CAAf","sourcesContent":["<template>\n<a-row type=\"flex\" justify=\"center\" v-if=\"employee.legal[0]\">\n    <a-col class=\"team_employee\" v-for=\"(item, index) in employee.legal\"\n           v-on:mouseover=\"employee.legal[index].active = true\"\n           v-on:mouseleave=\"employee.legal[index].active = false\"\n           :key=\"index\">\n      <img :src=\"item.picture\" class=\"image\">\n      <div class=\"team_employee_name\" v-if=\"employee.legal[index].active\">\n        <h1>{{ employee.legal[index].name }}</h1>\n      </div>\n    </a-col>\n  </a-row>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport {reactive, onMounted} from 'vue'\nexport default {\n  name: 'Team_legal',\n      setup() {\n    let employee = reactive({\n      legal: [],\n    })\n\n    onMounted(() => {\n      axios.get('http://127.0.0.1:8000/api/employee/legal')\n          .then((res) => {\n            for (let i = 0; i < res.data.length; i++) {\n                employee.legal.push(res.data[i])\n\n            }\n            employee.legal.sort((a, b) => {\n              return a['id'] - b['id']\n            })\n            for (let i = 0; i < employee.legal.length; i++) {\n              employee.legal[i]['picture'] = 'http://localhost:8000' + employee.legal[i]['picture']\n              employee.legal[i].active = false\n            }\n          })\n    })\n\n    return {employee}\n  }\n}\n</script>\n\n<style>\n.team_employee {\n  width: auto;\n  height: auto;\n  align-items: center;\n  justify-content: center;\n  max-width: 20.8vw;\n}\n\n.image {\n  width: 20.8vw;\n  height: auto;\n  max-width: 20.8vw;\n}\n\n.team_employee_name {\n  z-index: 2;\n  position: absolute;\n  bottom: 3vh;\n  left: 2vw;\n}\n\n.image:hover {\n  opacity: 0.5;\n}\n</style>"],"sourceRoot":""}]}