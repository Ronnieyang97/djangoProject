AC,CAAC,CAAC,CAAC,CAAC,CAAC;EACjB,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC;AACzC","file":"/Users/ronnie/PycharmProjects/djangoProject/appfront/src/components/team/Team_manager.vue","sourceRoot":"","sourcesContent":["<template>\n  <a-row type=\"flex\" justify=\"center\" v-if=\"true\">\n    <a-col class=\"team_employee\" v-for=\"(item, index) in employee.investment_manager\"\n           v-on:mouseover=\"employee.investment_manager[index].active = true\"\n           v-on:mouseleave=\"employee.investment_manager[index].active=false\"\n            :key=\"index\" :style=\"active_item(employee.investment_manager[index].active)\">\n      <img :src=\"item.picture\">\n      <div class=\"team_employee_name\"><h1>{{employee.investment_manager[index].name}}</h1></div>\n\n    </a-col>\n  </a-row>\n</template>\n<script>\n\nimport axios from \"axios\";\nimport {reactive, onMounted} from 'vue'\n\nexport default {\n  name: 'Team_manager',\n  setup() {\n    let employee = reactive({\n      investment_manager: [],\n    })\n    const active_item = (state) => {\n      if (state === true){\n        return {opacity: 0.5}\n      }\n      else{\n        return {opacity: 1}\n      }\n    }\n\n\n    onMounted(() => {\n      axios.get('http://127.0.0.1:8000/api/employee')\n          .then((res) => {\n            for (let i = 0; i < res.data.length; i++) {\n              if (res.data[i]['department'] === 'Investment manager') {\n                employee.investment_manager.push(res.data[i])\n              }\n            }\n            employee.investment_manager.sort((a, b) => {\n              return a['id'] - b['id']\n            })\n            for (let i = 0; i < employee.investment_manager.length; i++) {\n              employee.investment_manager[i]['picture'] = 'http://localhost:8000' + employee.investment_manager[i]['picture']\n              employee.investment_manager[i].active = false\n            }\n          })\n    })\n\n    return {employee, active_item}\n  }\n}\n</script>\n<style>\n.team_employee {\n  width: 20.6vw;\n  height: auto;\n  align-items: center;\n  justify-content: center;\n\n}\nimg {\n  width: auto;\n  height: auto;\n  max-width: 20.8vw;\n    z-index: 1;\n}\n.team_employee_name{\n  z-index: 2;position: absolute;bottom: 0;\n}\n\n</style>"]}]}